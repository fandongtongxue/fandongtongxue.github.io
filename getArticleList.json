---
layout: nil
---

[{% for post in site.posts limit:1000 %}
    {
        "title":"{{post.title}}",
        "url":"{{site.url}}{{post.url}}",
        "date":"{{post.date|date_to_string}}",
        "author":"{{post.author}}",
        "header_img":"{{post.header-img}}",
        "subtitle":"{{post.subtitle}}",
        "content":"{{{post.content}}}"
    }{% if forloop.last == false %},{% endif %}
{% endfor %}
]